<body class = "container">

<% if @event.errors.any? %>
    <% @event.errors.full_messages.each do |m| %>
    <p><%= m %></p>
    <% end %>
 <% end %>


<%= form_for @event, :url =>events_path do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= f.submit "Create" %>
<% end %>

</body>
